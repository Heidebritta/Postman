							"var headsign = pm.iterationData.get(\"headsign\").toString();",
							"var direction = pm.iterationData.get(\"directionId\");",
							"",
							"console.log(\"Test stopId \" +stopId+ \" stopName: \" +stopName+ \" routeId: \" + routeId + \" lastStop: \" + lastStop + \" headsign: \" +headsign + \" dir: \" +direction);",
							"",
							"if (arrDeptArray.length > 0) {",
							"//iterate through this and check all",
							"for (let i = 0; i < arrDeptArray.length; i++) {",
							"    pm.test(\"Trip \" +i+ \" StopId matches\", ()=> {",
							"        pm.expect(arrDeptArray[i].stopId).to.eql(stopId);",
							"        });",
							"        ",
							"    pm.test(\"Trip \" +i+ \" headsign matches\", function () {",
							"        pm.expect(arrDeptArray[i].tripHeadsign).to.eql(headsign);",
							"        });",
							"",
